.log-list {
    display: flex;
    flex-direction: column;
  }
  .log-item {
    margin: 10rpx;
  }
  .table {
    border: 0px solid darkgray;
    font-size:32rpx;
  }
  .tr {
    display: flex;
    width: 100%;
    justify-content: center;
    height: 2.6rem;
    align-items: center;
  }
  .td {
      width:40%;
      justify-content: center;
      text-align: center;
      font-size: 14px;
  }
  .bg-w{
    background: snow;
  }
  .bg-g{
    background: #E6F3F9;
  }
  .th {
    justify-content: center;
    background: #3366FF;
    color: #fff;
    display: flex;
    height: 2.6rem;
    align-items: center;
    font-size: 14px;
  }
  .name{
      width: 66%;
  }
  .number{
      width: 22%;
  }
  .pickersection{
    height:80rpx;
    line-height: 80rpx;
    padding:0 30rpx;
    display: flex;
    justify-content:flex-start;
  }
  .pickers{
    margin-right:30rpx;
  }
  .red{
    color: red
  }
  .pickersection,.weui-form-preview__label{
    font-size: 16px;
  }

  .weui-form-preview__bd, .weui-form-preview__label {
    color:#292421;
  }
  .style-one{
    width:98%;
    border-top:1px dotted #888;
    height: 1px;
    margin:10rpx 0;
  }
  .weui-form-preview__item{
    display: flex;
    justify-content: space-between;
    align-items: center;
  }
  .itemcon{
    width:45%;
    display: flex;
    justify-content: space-between;
    align-items: center;
  }