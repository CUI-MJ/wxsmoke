.log-list {
  display: flex;
  flex-direction: column;
}
.log-item {
  margin: 10rpx;
}
.table {
  border: 0px solid darkgray;
  font-size:32rpx;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2.6rem;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
    font-size: 14px;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 2.6rem;
  align-items: center;
  font-size: 14px;
}
.pickersection{
  height:80rpx;
  line-height: 80rpx;
  padding:0 30rpx;
  display: flex;
  justify-content: space-between
}
.red{
  color: red
}
.pickersection{
  font-size: 14px;
}